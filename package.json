{
  "name": "SpotSpot",
  "version": "0.0.2",
  "description": "A Spotify mini-player for macOS Edit",
  "main": "src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package --platform=darwin --arch=x64",
    "make": "electron-forge make"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "Spotify",
    "mini-player"
  ],
  "author": "Will Stone",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^3.0.1",
    "electron-prebuilt-compile": "1.6.8",
    "prettier": "^1.3.1"
  },
  "dependencies": {
    "electron-compile": "^6.4.1",
    "font-awesome": "^4.7.0",
    "minireset.css": "^0.0.3",
    "spotify-node-applescript": "^1.1.0",
    "textfit": "^2.3.1"
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "icon": "src/images/icon/icon.png.icns"
      },
      "electronWinstallerConfig": {
        "name": ""
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": ""
      }
    }
  }
}